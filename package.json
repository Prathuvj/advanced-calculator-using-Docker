  },
  "homepage": "https://github.com/prathuvj/advanced-calculator#readme",
  "keywords": [
    "calculator",
    "math",
    "node",
    "cli",
    "api",
    "docker"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mathjs": "^11.8.0",
    "readline-sync": "^1.4.10"
  },
  "engines": {
    "node": ">=14.0.0"
  }
